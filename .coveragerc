[run]
source = .
omit =
    */tests/*
    tests/*
    tests_strict/*
    .venv/*
    venv/*
    */site-packages/*
    **/__init__.py

[report]
show_missing = True
skip_covered = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.: 
    @overload
    @abstractmethod

[html]
directory = htmlcov
